<h1>会員詳細</h1>
<table>
    <thead>
        <tr>
            <th></th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>会員ID</td>
            <td><%= @member.id %></td>
        </tr>
        <tr>
            <td>氏名</td>
            <td><%= @member.last_name+@member.first_name %></td>
        </tr>
        <tr>
            <td>フリガナ</td>
            <td><%= @member.last_name_kana+@member.first_name_kana %></td>
        </tr>
        <tr>
            <td>ニックネーム</td>
            <td><%= @member.nickname %></td>
        </tr>
        <tr>
            <td>メールアドレス</td>
            <td><%= @member.email %></td>
        </tr>
        <tr>
            <td>会員ステータス</td>
            <td></td>
        </tr>
    </tbody>
</table>
<button type="button" class="btn btn-outline-dark">
    <%= link_to "編集する", edit_admin_member_path(@member.id) %>
</button>