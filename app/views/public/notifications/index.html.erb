<div class="container">
  <div class="row">
    <div class="col-9">
      <h3 class="text-center">通知一覧</h3>
      <% notifications = @notifications.where.not(visitor_id: current_member.id) %>
      <!--自分の投稿に対するいいね、コメントは通知に表示しない-->
    
      <% if notifications.exists? %>
        <%= render notifications %>
      <% else %>
       <p>通知はありません</p>
      <% end %>
    </div>
    
  </div>
</div>